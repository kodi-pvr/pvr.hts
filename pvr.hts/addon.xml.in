<addon
  id="pvr.hts"
  version="3.2.3"
  name="Tvheadend PVR Client Addon"
  provider-name="Adam Sutton, Sam Stenvall, Lars Op den Kamp, Kai Sommerfeld">
  <requires>
    <c-pluff version="0.1"/>
    <import addon="xbmc.pvr" version="5.1.1"/>
    <import addon="xbmc.codec" version="1.0.1"/>
